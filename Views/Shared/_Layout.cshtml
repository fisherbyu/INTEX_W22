<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BYU_EGYPT_INTEX</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BYU_EGYPT_INTEX.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
       <nav class="custom-navbar">
            <div class="custom-navbar-container container-fluid">
                <a class="custom-navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/img/buffalo-2-svgrepo-com.svg" />
                </a>
                <button class="custom-navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".custom-navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="custom-navbar-collapse collapse d-sm-inline-flex">
                    <ul class="custom-navbar-nav flex-grow-1">
                        <li class="custom-nav-item">
                            <a class="custom-nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="custom-nav-item">
                            <a class="custom-nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="custom-nav-item">
                            <a class="custom-nav-link" asp-area="" asp-controller="Home" asp-action="BurialData">Data</a>
                        </li>
                        <li class="custom-nav-item">
                            <a class="custom-nav-link" asp-area="" asp-controller="Home" asp-action="Supervised">Supervised Model 1</a>
                        </li>
                        <li class="custom-nav-item">
                            <a class="custom-nav-link" asp-area="" asp-controller="Home" asp-action="Supervised2">Supervised Model 2</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>


    </header>
    <br />
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - BYU_EGYPT_INTEX - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

